{
  "name": "vue-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "upload": "cross-env NODE_ENV=dev node deploy",
    "upload:test": "cross-env NODE_ENV=test node deploy",
    "upload:prod": "cross-env NODE_ENV=prod node deploy",
    "deploy": "npm run build && npm run upload",
    "deploy:test": "npm run build && npm run upload:test",
    "deploy:prod": "npm run build && npm run upload:prod"
  },
  "dependencies": {
    "ant-design-vue": "^1.6.4",
    "babel-polyfill": "^6.26.0",
    "core-js": "^2.6.11",
    "less": "^3.12.2",
    "less-loader": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "scp2": "^0.5.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
